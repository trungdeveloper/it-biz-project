{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0,
        "prettier/prettier": ["error", {
            "endOfLine":"auto",
            "tabWidth": 4
        }]
    }
}
